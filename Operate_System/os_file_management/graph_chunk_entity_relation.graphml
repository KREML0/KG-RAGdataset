      <data key="d0">System File</data>
      <data key="d1">category</data>
      <data key="d2">A system file is a file used by the operating system for essential functions and operations.</data>
      <data key="d3">chunk-ae42f9712539cb35032b3b324c78aeec</data>
      <data key="d4">unknown_source</data>
      <data key="d5">1761795242</data>
    </node>
    <node id="User File">
